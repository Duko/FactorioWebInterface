@page
@model FactorioWebInterface.Pages.Admin.AccountModel
@{ ViewData["Title"] = "Account";}

@section head{
    <link rel="stylesheet" href="~/css/account.css" asp-append-version="true" />
    <script type="module" src="~/js/account.js" asp-append-version="true"></script>
}

@Html.AntiForgeryToken()
<input type="hidden" name="__hasPassword" value="@Model.HasPassword" />
<input type="hidden" name="__passwordUpdated" value="@Model.PasswordUpdated" />
<input type="hidden" name="__accountError" value="@Model.Error" />
